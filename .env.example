# Train-Ticket 自动查询工具配置示例
# 复制此文件为 .env 并根据实际情况修改

# 服务器配置
TS_BASE_URL=http://139.196.152.44:31000
TS_USERNAME=fdse_microservice
TS_PASSWORD=111111

# 日期配置（如果不设置则使用当前日期）
# TS_DEFAULT_DATE=2023-07-01

# 地点配置
TS_PLACE_SHANG_HAI=Shang Hai
TS_PLACE_SU_ZHOU=Su Zhou
TS_PLACE_NAN_JING=Nan Jing

# 权重配置
TS_HIGHSPEED_WEIGHT=60
TS_NORMAL_WEIGHT=40

# 批量执行配置
TS_BATCH_COUNT=100
TS_BATCH_INTERVAL=1

# 压测配置
# 压测并发数
TS_STRESS_CONCURRENT=100
# 压测总请求数
TS_STRESS_COUNT=1000
# 压测场景，可选：high_speed, normal, food, parallel, pay, cancel, consign
TS_STRESS_SCENARIO=high_speed
# 请求超时时间(秒)
TS_STRESS_TIMEOUT=60
# 并发线程启动间隔(秒)
TS_STRESS_INTERVAL=0.1
# 可接受的错误率阈值
TS_STRESS_ERROR_RATE_THRESHOLD=0.1

# 日志级别: DEBUG, INFO, WARNING, ERROR, CRITICAL
TS_LOG_LEVEL=INFO 